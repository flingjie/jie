(:author "Jie"
 :deploy-dir "~/quicklisp/local-projects/jie/blog"
 :domain "http://127.0.0.1:8000/blog"
 :feeds ("lisp")
 :plugins ((mathjax)
           (sitemap)
           (static-pages)
          )
