(:author "Jie"
 :deploy-dir "~/workspace/blog"
 :domain "http://127.0.0.1:8000/blog"
 :feeds ("lisp")
 :plugins ((mathjax)
           (sitemap)
           (static-pages)
          )
  :routing ((:post           "posts/~a")
            (:tag-index      "tag/~a")
            (:month-index    "date/~a")
            (:numeric-index  "~d")
            (:archive-index  "~a")
            (:feed           "~a.xml")
            (:tag-feed       "tag/~a.xml"))
 :sitenav ((:url "http://127.0.0.1:8000/blog/archives.html" :name "Archives")
           (:url "http://127.0.0.1:8000" :name "Back to Home")
          )
 :title "Jie's Blog"
 :theme "simple")
